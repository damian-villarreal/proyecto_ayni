@using ayni.Sesiones


@{
    ViewBag.Title = "Ayni";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="">
    @{ if (Session["id"] == null)
        {
            @Html.Partial("../Partials/pv_NavScroll")
            @Html.Partial("../Partials/pv_IngresarModal")
        }
        else
        {
            @Html.Partial("../Partials/pv_UsuarioNav");
        }
    }


    @Html.Partial("../Partials/pv_HeaderImagen")

    <div class="container">
        @RenderBody()
    </div>
</div> 